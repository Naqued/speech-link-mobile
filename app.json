{
  "expo": {
    "name": "Speech Link",
    "slug": "speech-link-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A6FEA"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6fc68554-48d3-4b26-a2a9-c4fae0ab35d0"
      }
    },
    "newArchEnabled": true,
    "scheme": "com.naqued.speechlinkmobile",
    "owner": "naqued",
    "android": {
      "package": "com.naqued.speechlinkmobile",
      "permissions": [
        "INTERNET",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.naqued.speechlinkmobile",
      "supportsTablet": true
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "plugins": [
      "./app.plugin.js"
    ],
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-audio-recorder-player",
          "react-native-fs",
          "react-native-sound",
          "@expo/metro-runtime",
          "i18next",
          "socket.io-client",
          "@naqued/speech-link-mobile"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
